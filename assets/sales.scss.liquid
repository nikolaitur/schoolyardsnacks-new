/*Popup*/
.poplayer{
	position: fixed;
	background: rgba(0,0,0,0.7);
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	z-index: 90;
	cursor: pointer;
	display: none;
}
.popup{
	background: #fff;
	width: 90%;
	max-width: 900px;
	height: 80vh;
	transform: translate(-50% , 0);
	top: 10vh;
	left: 50%;
	position: fixed;
	z-index: 100;
	padding: 30px;
	overflow-y: auto;
	display: none;
}
.popclose{
	position: absolute;
	font-size: 30px;
	font-weight: 700;
	right: 10px;
	top: 10px;
	cursor: pointer;
	line-height: 1;
	text-transform: uppercase;
}
.popup .content > *{
	margin-bottom: 20px;
}
