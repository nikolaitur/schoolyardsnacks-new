/*Product bundle*/
.probundle-sec{
	padding: 10px 0;
}
.probundle-sec .inn{
	display: flex;
}
.probundle-sec .inn .figure{
	max-width: 50%;
	min-width: 50%;
}
.probundle-sec .inn .figure img{
	display: block;
	max-width: 115%;
	height: auto;
	margin-left:-15%;
}
.probundle-sec .inn .content{
	max-width: 50%;
	min-width: 50%;
	padding-left: 45px;
	padding-top: 65px;
	padding-bottom: 50px;
}
#text1, #text2{
    border: none;
    width: 28px;
    color: #152b5a;
    font-size: 15px;
    font-weight: 700;
    font-family: 'Proxima Nova';
}
.probundle-sec .inn .content .review{
	margin-bottom: 15px;
	display: flex;
	align-items: center;
	font-size: 14px;
	font-weight: 700;
	text-transform: uppercase;
	letter-spacing: 2px;
}
.probundle-sec .inn .content h4{
	font-size: 16px;
	text-transform: uppercase;
	margin-bottom: 20px;
}
.probundle-sec .inn .content ul.variations{
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
	margin-bottom: 10px;
}
.probundle-sec .inn .content ul.variations li{
	list-style: none;
	margin-bottom: 20px;
	max-width: calc(50% - 10px);
	min-width: calc(50% - 10px);
	position: relative;
}
.probundle-sec .inn .content ul.variations li input{
	position: absolute;
	opacity: 0;
	width: 0;
	height: 0;
	left: 0;
	top: 0;
}
.probundle-sec .inn .content ul.variations li input + label{
	display: block;
	position: relative;
	background: #d7f4f2;
	padding: 8px 8px 8px 40px;
	border-radius: 4px;
	font-size: 16px;
	font-weight: 700;
	line-height: 24px;
	cursor: pointer;
}
.probundle-sec .inn .content ul.variations li input + label:before{
	width: 24px;
	height: 24px;
	background: #fff;
	border-radius: 4px;
	position: absolute;
	content: '';
	left: 8px;
	top: 8px;
}
.probundle-sec .inn .content ul.variations li input:checked + label{
	background: #a0e3e0;
}
.probundle-sec .inn .content ul.variations li input:checked + label:after{
	width: 14px;
	height: 14px;
	background: #ff595a;
	border-radius: 4px;
	position: absolute;
	content: '';
	left: 13px;
	top: 13px;	
}
.probundle-sec .inn .content ul.variations li input + label strong{
	display: none;
	font-size: 13px;
	font-weight: 600;
	padding-top: 5px;
	line-height: 1;
}
.probundle-sec .inn .content ul.variations li input:checked + label strong{
	display: block;
}
.probundle-sec .inn .content ul.flavors{
	display: flex;
	margin-bottom: 30px;
}
.probundle-sec .inn .content ul.flavors li{
	margin-right: 22px;
	list-style: none;
	max-width: 100px;
}
.probundle-sec .inn .content ul.flavors li a{
	display: block;
	margin-bottom: 15px;
	border: 5px solid #FFF;
	border-radius: 50%;
}
.probundle-sec .inn .content ul.flavors li a.active{
	border: 5px solid #27ccc0;
}
.probundle-sec .inn .content ul.flavors li strong{
	display: block;
	font-size: 17px;
	font-weight: 700;
	text-align: center;
    padding-top: 10px;
}
.probundle-sec .inn .content p{
	margin-bottom: 15px;
}
.probundle-sec .inn .content .btns{
	display: flex;
	align-items: center;
	margin-bottom: 26px;
}
.probundle-sec .inn .content .btns .buybtn{
	margin-right: 15px;
}
.probundle-sec .inn .content .btns .price{
	font-size: 40px;
	font-weight: 700;
}
.probundle-sec .inn .content .btns .price span{
	text-decoration: line-through;
	color: #656980;
	font-size: 22px;
}

