/*Header*/
#header{
	background: #ffecd1;
}
#header .inn{
	display: flex;
	align-items: center;
	justify-content: space-between;
}
#header .inn .logo img{
	height: 90px;
	width: auto;
	display: block;
}
#header .inn .navi ul{
	display: flex;
	align-items: center;
}
#header .inn .navi ul li{
	list-style: none;
	margin-left: 60px;
}
#header .inn .navi ul li a{
	display: block;
	color: #152b5a;
	font-size: 16px;
	text-transform: uppercase;
	font-weight: 700;
}
/*Top bar*/
.top-bar{
	background: #fff3e1;
}
.top-bar .inn{
	display: flex;
	align-items: center;
	justify-content: center;
	position: relative;
	height: 90px;
}
.top-bar .inn p{
	color: #152b5a;
	font-weight: 700;
}
.top-bar .buybtn{
	position: absolute;
	right: 0;
	top: 22px;
}
/*Footer*/
#footer{
	background: #80d9d5;
}
#footer .inn{
	display: flex;
	align-items: center;
	justify-content:space-between;
	color: #152b5a;
	font-size: 14px;
	font-weight: 600;
	height: 110px;
}
#footer .inn a{
	display: inline-block;
	color: #152b5a;
	margin: 0 9px;
}
/*Tag line*/
.tagline{
	padding: 50px 0;
	line-height: 1;
	text-align: center;
}

/*Hero Section*/
.hero_sec{
	padding-bottom: 60px;
}
.hero_sec .inn{
	display: flex;
	justify-content: space-between;	
}
.pro-slider{
	max-width: 50%;
	min-width: 50%;
}
.pro-items{
	margin-bottom: 30px;
}
.pro-items .item img{
	border-radius: 5px;
}
.pro-items .slick-arrow{
	position: absolute;
	top: 50%;
	margin-top: -40px;
	width: 80px;
	height: 80px;
	text-indent: -9999px;
	border: 0;
	padding: 0;
	cursor: pointer;
	z-index: 50;
	background-repeat: no-repeat;
	background-color: transparent;
}
.pro-items .slick-arrow.slick-prev{
	left: -40px;
	background-image: url(icon-prev.png);
}
.pro-items .slick-arrow.slick-next{
	right: -40px;
	background-image: url(icon-next.png);
}
.pro-nav{
	/*padding: 0 22%;*/
}
.pro-nav .item{
	padding: 0 10px;
}
.pro-nav .item img{
	border: 3px solid #ffecd1;
	border-radius: 3px;
    cursor: pointer;
}
.pro-nav .item.slick-current img{
	border: 3px solid #ff595a;
}
.pro-info{
	max-width: calc(50% - 60px);
	min-width: calc(50% - 60px);
}
.pro-info .review{
	margin-bottom: 15px;
	display: flex;
	align-items: center;
	font-size: 15px;
	font-weight: 700;
	text-transform: uppercase;
}
.pro-info h2{
