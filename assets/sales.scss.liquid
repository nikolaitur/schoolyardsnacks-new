	align-items: center;
	font-size: 18px;
	font-weight: 700;
}
.pro-info .price span.actual{
	text-decoration: line-through;
	opacity: 0.35;
}
.pro-info .price span.discount{
	color: #27ccc0;
	padding: 0 20px;
}
.pro-info .price span.save{
	background: #27ccc0;
	padding: 0 15px;
	line-height: 28px;
	font-size: 14px;
	color: #fff;
	border-radius: 15px;
}
.pro-info .desc{
	padding-bottom: 20px;
}
.pro-info .desc p{
	margin-bottom: 20px;
}
.pro-info .desc ul li{
	list-style: none;
	margin-bottom: 20px;
	font-size: 20px;
	font-weight: 700;
	padding-left: 30px;
