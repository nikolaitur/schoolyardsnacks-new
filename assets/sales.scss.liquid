}
}
#shopify-section-sales-inst {
	.swiper__button--prev {
		display: block;
		position: absolute;
			left: -3px;
			top: 50%;
			transform: translate(-50%, -50%);
	}
	.swiper__button--next {
		display: block;
		position: absolute;
			right: -26px;
			top: 50%;
			transform: translate(-50%, -50%);
	}
