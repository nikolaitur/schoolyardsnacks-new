<div class="container">
  <h2 class="title">{{ section.settings.title }}</h2>
  <div class="image-text-wrap">
    <div class="text-wrap">
      {{ section.settings.content1 }}
    </div>
    <div class="image-wrap">
      <img src="{{ section.settings.image1 | img_url: 'master' }}" alt="{{ section.settings.image1.alt }}">
    </div>
  </div>
  <div class="image-text-wrap reverse">
    <div class="image-wrap">
      <img src="{{ section.settings.image2 | img_url: 'master' }}" alt="{{ section.settings.image2.alt }}">
    </div>
    <div class="text-wrap">
      {{ section.settings.content2 }}
    </div>    
  </div>
</div>
{% schema %}
{
  "name": "YT Lander 9",
  "class": "ytlander-9 ytlander-section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "image_picker",
      "id": "image1",
      "label": "Image 1"
    },
    {
      "type": "richtext",
      "id": "content1",
      "label": "Content 1"
    },
    {
      "type": "image_picker",
      "id": "image2",
      "label": "Image 2"
    },
    {
      "type": "richtext",
      "id": "content2",
      "label": "Content 2"
    }
  ]
}
{% endschema %}