<header class="header header--secondary">
  <div class="header-promo"><p>Today Only! Save 40% On Your First Order + Free Shipping</p></div>
  <div class="header-main">
    <div class="container--fluid">
      <div class="header-logo">
        <a href="/{% if article.handle contains '10-best-keto' %}?ut=articleD{% endif %}" title="{{ shop.name }}" aria-label="{{ shop.name }}">
        <!-- <img src="{{ 'logo.png' | asset_img_url: '280x' }}" alt="{{ shop.name }}" /> -->
          <img src="data:image/png;base64," alt="{{ shop.name }}" />
        </a>
      </div>
      <div class="header-text">Today Only! Save 40% On Your First Order + Free Shipping</div>
      <div class="header-action">
        <button class="btn btn-unlock-offer">Unlock Offer</button>
      </div>
    </div>
  </div>
</header>