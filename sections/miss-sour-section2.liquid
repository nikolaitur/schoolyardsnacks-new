  <div class="image-slider1 image-slider social hide--desktop">
    <div class="swiper-wrapper">
      <div class="swiper-slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0048/4330/7123/files/edge_bc8668db-9fd3-407e-838c-1241b0a4ff1a.png?v=1632424858"
          alt="review image"
        >
      </div>

      <div class="swiper-slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0048/4330/7123/files/jewel_edbd4ab9-f8db-430f-be9a-e976b2e12cff.png?v=1632424858"
          alt="review image"
        >
      </div>
    </div>
    <div class="swiper__button swiper__button--next swiper__button--next1">
      {% include 'icon', handle: 'next' %}
    </div>
    <div class="swiper__button swiper__button--prev swiper__button--prev1">
      {% include 'icon', handle: 'prev' %}
    </div>
  </div>

  <a
    href="https://schoolyardsnacks.com/products/keto-cheese-puffs-6-family-size-bags"
    class="btn btn-flavor-select hide--desktop"
    >Get Yours Now</a
  >
