    const countDown = new Date().getTime() + 3*3600*1000,
    x = setInterval(function() {    

      const now = new Date().getTime(),
            distance = countDown - now;

        if(distance < 0) {
          countDown = new Date().getTime() + 3*3600*1000
        }


        $('.hours').each(function(){
          
          $(this).text(Math.floor((distance % (day)) / (hour)));
        });
