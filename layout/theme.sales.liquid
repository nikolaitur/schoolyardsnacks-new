	});
	var now = new Date();
var utcDate = new Date(now.toUTCString());
utcDate.setHours(now.getHours() - 17);
var usDate = new Date(utcDate);
var week_array = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"];
var mon_array = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
switch(usDate.getDay()){
	case 0:
		usDate.setDate(usDate.getDate() + 2);
		break;
	case 1:
		usDate.setDate(usDate.getDate() + 2);
		break;
	case 2:
		usDate.setDate(usDate.getDate() + 2);
		break;
	case 3:
		usDate.setDate(usDate.getDate() + 2);
		break;
	case 4:
		usDate.setDate(usDate.getDate() + 4);
		break;
	case 5:
		usDate.setDate(usDate.getDate() + 4);
		break;
	case 6:
		usDate.setDate(usDate.getDate() + 3);
		break;
}
var targetString = "Get it by";
targetString += " <strong>" + week_array[usDate.getDay()];
targetString += ", " + mon_array[usDate.getMonth()];

var suffix_str = 'th</strong>';
  if(usDate.getDate() == 1) suffix_str = 'st</strong>';
if(usDate.getDate() == 2) suffix_str = 'nd</strong>';
else if(usDate.getDate() == 3) suffix_str = 'rd</strong>';
console.log("suffix:-"+usDate.getDate()+"-: " + suffix_str);
targetString += " " + usDate.getDate() + suffix_str;
targetString += " when you order within ";
if(usDate.getMinutes() != 0) targetString += "<strong>" +  (23 - usDate.getHours()) + "hr</strong> ";
else targetString += "<strong>" + (24 - usDate.getHours()) + "hr</strong>  ";
 targetString += "<strong>" +  (60 - usDate.getHours()) + "min</strong>";
	$(".time_message").html(targetString);
  $("#shopify-section-sales-how-works .work-items > .item:nth-child(2) .content p").html(targetString);
	
	$('#footer .navi a, .facts-slider .item .content .btns a').click(function(){
		var alink = $(this).attr('href');
		$('.poplayer').show();
		$(alink).show();
		return false;
	});
	
	$('.poplayer2').click(function(){
		$(this).hide();
		$('.popup').hide();
	});
	
	$('.popclose').click(function(){
		$('.poplayer2').hide();
		$('.popup').hide();
		return false;
	});
	
	$('#header .inn .navi ul li a').click(function(){
